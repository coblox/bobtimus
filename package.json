{
  "name": "bobtimus",
  "version": "1.0.0",
  "description": "an autobot that automatically handles swaps as Bob",
  "main": "index.js",
  "scripts": {
    "check": "tsc",
    "build": "mkdir -p ./gen && json2ts -i ./schema/siren.schema.json -o ./gen/siren.d.ts",
    "start": "yarn run build && ts-node -- ./src/index.ts",
    "start-dev": "yarn run build && nodemon --exec ./node_modules/.bin/ts-node -- ./src/index.ts",
    "format": "prettier --write '{src,stubs}/**/*.ts' '*.json' && tslint -p . --fix",
    "lint": "tslint -p .",
    "prettier": "prettier --check '{src,stubs}/**/*.ts' '*.json'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coblox/bobtimus.git"
  },
  "keywords": [],
  "author": "The CoBloX Team <team@coblox.tech>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/coblox/bobtimus/issues"
  },
  "homepage": "https://github.com/coblox/bobtimus#readme",
  "dependencies": {
    "@badrap/result": "^0.2.4",
    "big.js": "^5.2.2",
    "fs": "^0.0.1-security",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "@types/big.js": "^4.0.5",
    "@types/request": "^2.48.1",
    "@types/request-promise-native": "^1.0.16",
    "json-schema-to-typescript": "^6.1.3",
    "nodemon": "^1.19.0",
    "ts-node": "^8.1.0",
    "tsc": "^1.20150623.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  }
}
